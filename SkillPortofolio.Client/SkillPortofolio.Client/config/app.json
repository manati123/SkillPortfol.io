{
    "$schema": "../node_modules/sp-build-tasks/schema/v1/sppp.json",
    "spFolder": "SiteAssets/SkillPortofolio",
    "distFolder": "./dist",
    "copyAssetsMap": [{
            "name": "PnPjs",
            "src": [
                "./node_modules/@pnp/pnpjs/dist/pnpjs.es5.umd.bundle.min.js",
                "./node_modules/@pnp/pnpjs/dist/pnpjs.es5.umd.bundle.min.js.map"
            ],
            "dist": "./dist/libs"
        },
        {
            "name": "React",
            "src": [
                "./node_modules/react/umd/react.production.min.js",
                "./node_modules/react-dom/umd/react-dom.production.min.js"
            ],
            "dist": "./dist/libs"
        },
        {
            "name": "Office UI Fabric",
            "src": [
                "./node_modules/office-ui-fabric-react/dist/office-ui-fabric-react.min.js",
                "./node_modules/office-ui-fabric-react/dist/office-ui-fabric-react.min.js.map"
            ],
            "dist": "./dist/libs"
        },
        {
            "name": "Office UI Fabric Fonts",
            "src": [
                "./node_modules/@uifabric/icons/fonts"
            ],
            "dist": "./dist"
        },
        {
            "name": "Office UI Fabric Styles",
            "src": [
                "./node_modules/office-ui-fabric-react/dist/css/fabric.min.css"
            ],
            "dist": "./dist/styles"
        },
        {
            "name": "React",
            "src": [
                "./node_modules/react/umd/react.production.min.js",
                "./node_modules/react-dom/umd/react-dom.production.min.js"
            ],
            "dist": "./dist/libs"
        },
        {
            "name": "Images",
            "src": [
                "./src/images"
            ],
            "dist": "./dist"
        }
    ],
    "webpackItemsMap": [{
            "name": "Polyfills",
            "entry": "./src/scripts/utils/polyfills.ts",
            "target": "polyfills.js",
            "includePolyfills": false
        },
        {
            "name": "journey.cewp",
            "entry": "./src/scripts/components/JourneyPage/JourneyPage.cewp.ts",
            "target": "JourneyPage.cewp.js",
            "includePolyfills": false
        },
        {
            "name": "mainpage.cewp",
            "entry": "./src/scripts/components/MainPage/MainPage.cewp.ts",
            "target": "MainPage.cewp.js",
            "includePolyfills": false
        },
        {
            "name": "coachdashboard.cewp",
            "entry": "./src/scripts/components/CoachDashboard/CoachDashboard.cewp.ts",
            "target": "CoachDashboard.cewp.js",
            "includePolyfills": false
        },
        {
            "name": "coachjourneypage.cewp",
            "entry": "./src/scripts/components/CoachJourneyPage/CoachJourneyPage.cewp.ts",
            "target": "CoachJourneyPage.cewp.js",
            "includePolyfills": false
        }
    ]
}